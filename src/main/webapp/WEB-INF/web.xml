    <servlet-mapping>
        <servlet-name>SaludoServlet</servlet-name>
        <url-pattern>/SaludoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SaludoServlet2</servlet-name>
        <url-pattern>/SaludoServlet2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SaludoServlet3</servlet-name>
        <url-pattern>/SaludoServlet3</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SaludoServlet4</servlet-name>
        <url-pattern>/SaludoServlet4</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TutoriasServlet</servlet-name>
        <url-pattern>/TutoriasServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TutoriasServlet2</servlet-name>
        <url-pattern>/TutoriasServlet2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RespuestaXML</servlet-name>
        <url-pattern>/RespuestaXML</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
