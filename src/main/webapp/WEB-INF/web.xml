    </servlet>
    <servlet>
        <servlet-name>SaludoServlet4</servlet-name>
        <servlet-class>controladores.SaludoServlet4</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TutoriasServlet</servlet-name>
        <servlet-class>controladores.TutoriasServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TutoriasServlet2</servlet-name>
        <servlet-class>datos.TutoriasServlet2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RespuestaXML</servlet-name>
        <servlet-class>controladores.RespuestaXML</servlet-class>
